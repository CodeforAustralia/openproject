<script type="text/template" id="planning-element-headers-template">
  {{ _.each(collection.models, function(pe, i){ }}
    {{ if(i == collection.size() - 1){ }}
    <tr class="tl-last-row" style="display:none" data-parent-project="{{= parent_id }}">
    {{ } else { }}
    <tr style="display:none" data-parent-project="{{= parent_id }}">
    {{ } }}
      <td class="tl-first-column tl-indent-1" data-cell-planning-element-id="{{= pe.id }}">
        <!-- Note: we are using element ids here which is bad -->
        <span class="tl-word-ellipsis">
          <span>
            <a href="/work_packages/{{= pe.id }}" class="tl-discreet-link" data-modal="" title="{{= pe.subject }}">{{= pe.get('subject') }}</a>
          </span>
        </span>
      </td>
    </tr>
  {{ }); }}
</script>